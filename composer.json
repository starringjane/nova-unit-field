{
    "name": "starring-jane/nova-unit-field",
    "description": "A Laravel Nova field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1"
    },
    "autoload": {
        "psr-4": {
            "StarringJane\\NovaUnitField\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "StarringJane\\NovaUnitField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
